{
  "shortcuts": {
    "general": {
      "eav_attribute_set": {
        "delete": [
          "general/general/global-data",
          "general/general/move-files",
          "general/eav_attribute_set/delete",
          "general/general/archive-artefacts"
        ],
        "replace": [
          "general/general/global-data",
          "general/general/move-files",
          "general/eav_attribute_set/replace",
          "general/general/archive-artefacts"
        ],
        "add-update": [
          "general/general/global-data",
          "general/general/move-files",
          "general/eav_attribute_set/add-update",
          "general/general/archive-artefacts"
        ]
      }
    }
  },
  "operations": {
    "general": {
      "eav_attribute_set": [
        {
          "name": "delete",
          "plugins": [
            {
              "id": "import.plugin.global.data"
            },
            {
              "id": "import.plugin.subject",
              "subjects": [
                {
                  "id": "import_attribute_set.subject.bunch",
                  "file-resolver": {
                    "prefix": "attribute-set-import"
                  },
                  "observers": [
                    {
                      "import": [
                        "import_attribute_set.observer.clear.attribute.set"
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "name": "replace",
          "plugins": [
            {
              "id": "import.plugin.global.data"
            },
            {
              "id": "import.plugin.subject",
              "subjects": [
                {
                  "id": "import_attribute_set.subject.bunch",
                  "file-resolver": {
                    "prefix": "attribute-set-import"
                  },
                  "observers": [
                    {
                      "import": [
                        "import_attribute_set.observer.composite.replace"
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "name": "add-update",
          "plugins": [
            {
              "id": "import.plugin.subject",
              "subjects": [
                {
                  "id": "import_attribute_set.subject.bunch",
                  "file-resolver": {
                    "prefix": "attribute-set-import"
                  },
                  "params": [
                    {
                      "copy-parent-on-update": false
                    }
                  ],
                  "observers": [
                    {
                      "import": [
                        "import_attribute_set.observer.composite.add_update"
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
  }
}